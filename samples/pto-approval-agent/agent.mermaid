flowchart TD
    step__done["_done"]:::stepStyle
    step_handle_approval["handle_approval"]:::stepStyle
    step_process_pto_request["process_pto_request"]:::stepStyle
    event_StopEvent([<p>StopEvent</p>]):::stopEventStyle
    event_ProcessApprovalEvent([<p>ProcessApprovalEvent</p>]):::defaultEventStyle
    event_PTORequestEvent([<p>PTORequestEvent</p>]):::defaultEventStyle
